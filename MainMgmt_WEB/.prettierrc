{
  "printWidth": 200,
  "eslintIntegration": true,
  "stylelintIntegration": true,
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "bracketSpacing": true,
  "arrowParens": "always",
  "htmlWhitespaceSensitivity": "ignore",
  "javascript.format.insertSpaceBeforeFunctionParenthesis": true,
  "files.insertFinalNewline": true,
  "useTabs": false,
  "endOfLine": "auto",
  "ignorePath": ".gnore",
  "trailingComma": "none"
}
